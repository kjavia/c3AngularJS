%div{'ng-controller' => 'Example2Controller as vm'}
  %h3 Example 2
  %p
    Example with mouse event handlers. Events supported on data
    items are onmouseover, onmouseout, onselected, onclick.
  %div{'c3-chart' => true, id: '{{vm.elementId}}', raw: 'vm.data', group: '{{vm.group}}'}
  %p click : {{vm.clickItem | json}}
  %p mouseover : {{vm.mouseOverItem | json}}
  %p mouseout : {{vm.mouseOutItem | json}}
  %p selected : {{vm.selectedItem | json}}
  %p unselected : {{vm.unSelectedItem | json}}

  %div{'ng-include' => true, src: '"tabs.html"'}
